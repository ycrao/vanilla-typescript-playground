{
  "name": "vanilla-typescript-playground",
  "version": "1.0.0",
  "description": "write some code play with typescript!",
  "main": "index.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "type-check": "tsc --noEmit -p tsconfig.json --composite false",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "https+git://github.com/ycrao/vanilla-typescript-playground.git"
  },
  "keywords": [
    "play",
    "typescript",
    "vanilla",
    "playground"
  ],
  "author": "ycrao",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "parcel": "^2.8.3"
  }
}
